            <Label text="Всички формуляри на потребител">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <ComboBox fx:id="userBox" prefHeight="27.0" prefWidth="181.0" promptText="Избери потребител">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </ComboBox>
            <Label text="Или">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="userId" maxWidth="-Infinity" onKeyTyped="#onUserId" prefHeight="27.0" prefWidth="131.0" promptText="ID на потребителя" />
