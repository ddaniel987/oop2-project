<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="263.0" prefWidth="328.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.oop2proekt2.ChangeRoleController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="-2.0" layoutY="-2.0" prefHeight="312.0" prefWidth="330.0">
         <children>
            <Label text="Промяна на ролята на потребител">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <ComboBox fx:id="userBox" prefHeight="27.0" prefWidth="181.0" promptText="Избери потребител">
               <VBox.margin>
                  <Insets top="35.0" />
               </VBox.margin>
            </ComboBox>
            <Label text="Или">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="userId" maxWidth="-Infinity" onKeyTyped="#onUserId" prefHeight="27.0" prefWidth="131.0" promptText="ID на потребител" />
            <ComboBox fx:id="roleBox" layoutX="85.0" layoutY="83.0" prefHeight="27.0" prefWidth="181.0" promptText="Роля">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </ComboBox>
            <Button fx:id="go" disable="true" mnemonicParsing="false" onAction="#onGoBtn" prefHeight="27.0" prefWidth="86.0" text="Запиши">
               <VBox.margin>
                  <Insets top="50.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
